{
  "name": "goldengate-api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "bun run src/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun run src/db/seeds/index.ts",
    "db:shell": "docker-compose exec postgres psql -U postgres -d goldengate",
    "db:shell:test": "docker-compose exec postgres-test psql -U postgres -d goldengate_test",
    "contractors:import": "bun run scripts/import-contractors.ts",
    "contractors:sync": "bun run src/services/contractors/sync.ts",
    "contractors:improve-mappings": "bun run src/scripts/improve-uei-mappings.ts",
    "etl:snowflake": "bun run src/scripts/etl/snowflake-loader-v2.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:e2e": "NODE_ENV=test bun test e2e/",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src/",
    "lint:fix": "biome check --apply src/",
    "setup:dev": "../scripts/dev.sh setup",
    "health": "../scripts/dev.sh health"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.63.1",
    "@better-auth/client": "^0.0.2-alpha.3",
    "@elysiajs/bearer": "^1.0.2",
    "@elysiajs/cors": "^1.0.2",
    "@elysiajs/jwt": "^1.3.2",
    "@elysiajs/swagger": "^1.3.1",
    "@modelcontextprotocol/sdk": "^1.17.1",
    "@types/glob": "^9.0.0",
    "argon2": "^0.31.2",
    "better-auth": "^1.3.4",
    "csv-parse": "^6.1.0",
    "drizzle-orm": "^0.29.4",
    "elysia": "^1.0.21",
    "glob": "^11.0.3",
    "jose": "^6.0.12",
    "nanoid": "^5.0.4",
    "postgres": "^3.4.3",
    "snowflake-sdk": "^2.1.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "latest",
    "aws-sdk": "^2.1692.0",
    "drizzle-kit": "^0.20.17",
    "mock-aws-s3": "^4.0.2",
    "nock": "^14.0.10",
    "typescript": "^5.3.3"
  }
}
